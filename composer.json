{
    "name": "emanueleminotto/json-schema-faker",
    "type": "library",
    "description": "Faker provider for JSON Schema",
    "keywords": [
        "json",
        "schema",
        "faker"
    ],
    "license": "MIT",
    "require": {
        "php": "~7.1",
        "fzaninotto/faker": "^1.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "opis/json-schema": "^1.0",
        "phpunit/phpunit": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "EmanueleMinotto\\JsonSchemaFaker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmanueleMinotto\\JsonSchemaFaker\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --repeat 10",
        "check-style": "php-cs-fixer fix --dry-run --diff",
        "fix-style": "php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    }
}
